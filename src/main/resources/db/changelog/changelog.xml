<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <include file="sql/V1_create_contractor_role.sql" relativeToChangelogFile="true"/>
    <include file="sql/V2_create_currency.sql" relativeToChangelogFile="true"/>
    <include file="sql/V3_create_deal_status.sql" relativeToChangelogFile="true"/>
    <include file="sql/V4_create_deal_type.sql" relativeToChangelogFile="true"/>
    <include file="sql/V5_create_deal.sql" relativeToChangelogFile="true"/>
