    <changeSet id="load_contractor_role" author="anailina">
        <loadData
          tableName="contractor_role"
          file="db/changelog/data/contractor_role.csv"
          separator=";"
        />
    </changeSet>
    <changeSet id="load_currency-data" author="anailina">
        <loadData
          tableName="currency"
          file="db/changelog/data/currency.csv"
          separator=";"
        />
    </changeSet>
    <changeSet id="load_deal_status" author="anailina">
        <loadData
          tableName="deal_status"
          file="db/changelog/data/deal_status.csv"
          separator=";"
        />
    </changeSet>
    <changeSet id="load_deal_type" author="anailina">
        <loadData
          tableName="deal_status"
          file="db/changelog/data/deal_type.csv"
          separator=";"
        />
    </changeSet>
</databaseChangeLog>